<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.inno72.mapper.Inno72UserMapper">
    <resultMap id="BaseResultMap" type="com.inno72.model.Inno72User">
        <!--
          WARNING - @mbg.generated
        -->
        <id column="id" jdbcType="BIGINT" property="id"/>
        <result column="uName" jdbcType="VARCHAR" property="uName"/>
        <result column="uPassword" jdbcType="VARCHAR" property="uPassword"/>
    </resultMap>
    <sql id="Base_Column">
  id, uName , uPassword
  </sql>

    <select id="selectByUsername" resultMap="BaseResultMap">
        select
        <include refid="Base_Column"/>
        from inno72_user where uName = #{uName}
    </select>
</mapper>